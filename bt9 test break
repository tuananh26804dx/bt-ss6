#include <stdio.h>

int main() {
    int x = 5;

    switch(x) {
        case 5:
            printf("Truong hop x = 5");			
        case 2:
            printf("\nTruong hop x = 2");
            break;
        case 1:
            printf("\nTruong hop x = 1");
            break;
        default:
            printf("\nChay vao default");
    }

    return 0;
}
